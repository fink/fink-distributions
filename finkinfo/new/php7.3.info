DefaultScript: debhelper
PatchScript: <<
# Remove plugins we can't build yet, missing deps, etc, has to happen before all
# dh calls
for ext in interbase firebird; do \
	rm -rf debian/*${ext}.* debian/rules.d/ext-${ext}.mk; \
	sed -i "/${ext}/d" debian/rules; \

	sed -i "/^Package: %N-${ext}/,/^Package:/d" debian/control; \
done

perl -pi -e 's,0012-php-5.4.7-libdb.patch,#0012-php-5.4.7-libdb.patch,g' debian/patches/series

dh_quilt_patch

%{default_script}

# Find dbx is dbx not dbx.y
perl -pi -e 's,db5.0,db5,g' configure ext/dba/config.m4

# zlib is a system lib on macOS
sdkpath=`xcrun --show-sdk-path`; \
perl -pi -e "s,--with-zlib=%p,--with-zlib=${sdkpath}/usr,g" debian/rules; \
perl -pi -e "s,--with-zlib-dir=%p,--with-zlib-dir=${sdkpath}/usr,g" debian/rules debian/rules.d/ext-zip.mk debian/rules.d/ext-mysql.mk

# fix mysql.sock location
perl -pi -e 's,--enable-mysqlnd,--with-mysql-sock=%p/var/run/mysqld/mysqld.sock --enable-mysqlnd,g' debian/rules.d/ext-mysql.mk

# macOS uses _www instead of www-data
perl -pi -e 's,www-data,_www,g' debian/php-fpm.*

perl -pi -e 's,amd64,darwin-x86_64,g' debian/rules
perl -pi -e 's,readlink,greadlink,g' debian/rules
perl -pi -e 's,getconf LFS_CFLAGS,echo "",g' debian/rules
perl -pi -e 's,export DEB_LDFLAGS_MAINT_APPEND,#export DEB_LDFLAGS_MAINT_APPEND,g' debian/rules

# No idea why I ened to disable cli here, Debian does not, but linking
# doesn't not work on cli for some reason
perl -pi -e 's,--enable-embed --enable-cli,--enable-embed --disable-cli,g' debian/rules

# darwin sendmail is /usr/sbin/sendmail
perl -pi -e 's,%p/sbin/sendmail,/usr/sbin/sendmail,g' debian/rules

# fix include dir in config
perl -pi -e 's,/usr/share/php,%p/share/php,g' php.ini-* configure.ac
# darwin X11 dir
perl -pi -e 's,with-xpm-dir=.*,with-xpm-dir=/opt/X11 \\,g' debian/rules.d/ext-gd.mk
perl -pi -e 's,i/X11,i/include,g' ext/gd/config.m4 configure

# Specify path to iconv
perl -pi -e 's;with-iconv=shared;with-iconv=shared,%p;g' debian/rules.d/ext-common.mk

# fink uses readline instead of libedit
perl -pi -e 's,with-libedit,with-readline,g' debian/rules debian/rules.d/ext-readline.mk

# use GNU tools
perl -pi -e 's,mv -u,gmv -u,g' debian/rules
perl -pi -e 's,rmdir,grmdir,g' debian/php-module.postrm.in debian/php-module.preinst.in debian/php-sapi.postrm

# fink libzip isn't in include directly
perl -pi -e 's,with-libzip=%p,with-libzip=%p/lib/libzip,g' debian/rules.d/ext-zip.mk

# system-tzdata doesn't work on darwin, disable it
perl -pi -e 's,with-system-tzdata,without-system-tzdata,g' debian/rules
# fink doesn't have argon
sed -i '/argon2/d' debian/rules

# change run dir
perl -pi -e 's,/run/php,%p/var/run/php,g' debian/php-fpm* debian/rules.d/prepare-fpm-pools.mk

# Userdir location
perl -pi -e 's,home,Users,g' debian/php.conf
perl -pi -e 's,public_html,Sites,g' debian/php.conf

# Touch file so build doesn't fail is tests aren't run.  This is an issue as
# it changes the deb file, so this is not a solution it's just to finish the
# build and come back to fix properly.
touch test-results.txt
<<
