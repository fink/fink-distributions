+-{
+-    $SED -n '/^# '$PROGRAM' (GNU /,/# warranty; / {
+-        s/^# //
+-	s/^# *$//
+-        s/\((C)\)[ 0-9,-]*\( [1-9][0-9]*\)/\1\2/
+-        p
+-     }' < "$progpath"
+-     exit $?
+-}
