+-*/
+-static apr_datum_t dav_fs_build_key(apr_pool_t *p,
+-                                    const dav_resource *resource)
+-{
+-    const char *file = dav_fs_pathname(resource);
+-    apr_datum_t key;
+-    apr_finfo_t finfo;
+-    apr_status_t rv;
+-
+-    /* ### use lstat() ?? */
+-    /*
