+  void Smiles::freeSmiles()  {
+    if (ver)       delete[] ver;
+    if (plain)     delete[] plain;
+    if (canonical) delete[] canonical;
+    ver       = NULL; //!< software version
+    plain     = NULL; //!< plain smiles
