+    get_atom_from_cif ( atom_2,mmCIFLoop,MMCIF_TAG_ATOM_ID_2,
+                        angleNo,atoms,rc );
+    get_atom_from_cif ( atom_3,mmCIFLoop,MMCIF_TAG_ATOM_ID_3,
+                        angleNo,atoms,rc );
+
+    mmCIFLoop->CopyReal(angle    ,MMCIF_TAG_VALUE_ANGLE    ,angleNo,rc);
