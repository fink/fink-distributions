  (cd src/interfaces/jdbc; make)
<<
InstallScript: <<
  (cd src/interfaces/jdbc; make install DESTDIR=%d)
  install -d -m 755 %i/lib/postgresql
  mv %i/share/postgresql/java %i/lib/postgresql/java
  rmdir %i/share/postgresql
  rmdir %i/share
