InstallScript: <<
#!/bin/sh -ex
  mkdir -p %i/var/lib/php5
  chmod 01733 %i/var/lib/php5

  mkdir -p %d/`apxs2 -q LIBEXECDIR`
  mkdir -p %i/etc/apache2/mods-available
  cp %b/apache2-build/libs/libphp5.so \
          %d/`apxs2 -q LIBEXECDIR`/
  cp %b/fink/php5.load \
          %i/etc/apache2/mods-available/php5.load
  cp %b/fink/php5.conf \
          %i/etc/apache2/mods-available/php5.conf

  ### sanitize php.ini file
  mkdir -p %b/configs
  mkdir -p %b/examples
  cat php.ini-dist | tr "\t" " " > %b/configs/php.ini-dist
  cat php.ini-dist | tr "\t" " " | sed -e'/memory_limit =/ s/8M/32M/g' > %b/configs/php.ini-dist.cli
  cat php.ini-dist | tr "\t" " " > %b/examples/php.ini-dist
  cat php.ini-recommended | tr "\t" " " > %b/examples/php.ini-recommended
  cat php.ini-paranoid | tr "\t" " " > %b/examples/php.ini-paranoid

  # install the apache modules' files
  cd %b/apache2-build && make install-headers install-build install-modules install-programs INSTALL_ROOT=%d
  # remove netware and win32 headers that we don't want
  cd %i/include/php5/ && \
       rm -f TSRM/readdir.h \
             TSRM/tsrm_config.{nw,w32}.h \
             TSRM/tsrm_{nw,win32}.h \
             Zend/zend_config.{nw,w32}.h \
             main/config.{nw,w32}.h \
             main/win95nt.h

  # generate the config snippets for various php
  # modules from the templates.
  cd %b
  mkdir -p %i/etc/php5/conf.d
  cat fink/modulelist fink/extramodulelist | while read package extname dsoname; do \
          if [ -z "$dsoname" ]; then dsoname=$package; fi; \
          sed -e"s/@extname@/$extname/g; s/@dsoname@/$dsoname/g" \
            < fink/php5-module.ini \
            > %i/etc/php5/conf.d/${dsoname}.ini; \
  done

  # install PEAR
  cp -r %b/pear-build/* %d

  # install CGI
  mkdir -p %i/lib/cgi-bin
  mkdir -p %i/bin
  mkdir -p %i/share/man/man1
  cp %b/cgi-build/sapi/cgi/cgi-bin.php5 %i/lib/cgi-bin/php5
  cp %b/cgi-build/sapi/cgi/usr.bin.php5-cgi %i/bin/php5-cgi
  cp %b/cli-build/sapi/cli/php.1 %i/share/man/man1/php5-cgi.1

  # install CLI
  mkdir -p %i/bin
  mkdir -p %i/share/man/man1
  cp %b/cli-build/sapi/cli/php %i/bin/php5
  cp %b/cli-build/sapi/cli/php.1 %i/share/man/man1/php5.1

  for i in php-config phpize; do \
        mv %i/bin/$i %i/bin/"$i"5; \
        mv %i/share/man/man1/"$i".1 %i/share/man/man1/"$i"5.1; \
  done

  mkdir -p %i/lib/php5
  install -m755 %b/fink/maxlifetime %i/lib/php5

  # install crontab
  mkdir -p %i/etc/cron.d
  install -m644 %b/fink/php5.cron %i/etc/cron.d/php5

  ### installing default config files
  mkdir -p %i/etc/php5/apache2
  mkdir -p %i/etc/php5/cli
  mkdir -p %i/etc/php5/cgi
  install -m 644 %b/configs/php.ini-dist %i/etc/php5/apache2/php.ini
  install -m 644 %b/configs/php.ini-dist.cli %i/etc/php5/cli/php.ini
  install -m 644 %b/configs/php.ini-dist %i/etc/php5/cgi/php.ini

  ### Make needed links
  ln -s %p/lib/php5/20060613-zts %i/lib/php5/libexec

  ### Remove un needed files
  rm -rf %i/lib/php5/20060613-zts/*.a
<<
