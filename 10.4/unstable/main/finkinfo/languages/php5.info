CompileScript: <<
#!/bin/sh -ex
  ### Configure for apache2
  if [ -d %b/apache2-build ]; then rm -rf %b/apache2-build; fi
  mkdir -p %b/apache2-build
  cd %b/apache2-build && \
  PGSQL_INCLUDE=`%p/bin/pg_config-8.3 --includedir` EXTRA_LIBS="-L%p/lib/system-openssl/lib" YACC="/usr/bin/bison -y" ../configure \
  --with-apxs2=%p/bin/apxs2 \
  --with-config-file-path=%p/etc/php5/apache2 \
  --with-config-file-scan-dir=%p/etc/php5/conf.d \
  %c \
  --without-mm \
  --with-curl=shared,%p \
  --with-zlib-dir=/usr \
  --with-gd=shared,%p \
  --enable-gd-native-ttf \
  --with-jpeg-dir=%p \
  --with-xpm-dir=shared,/usr/X11R6 \
  --with-png-dir=%p \
  --with-freetype-dir=%p/lib/freetype219 \
  --with-imap=shared,%p \
  --with-imap-ssl=%p/lib/system-openssl \
  --with-ttf=shared,%p \
  --with-t1lib=%p \
  --with-ldap=shared,%p \
  --with-ldap-sasl=%p \
  --with-mcrypt=shared,%p \
  --with-mhash=shared,%p \
  --with-mysql=shared,%p \
  --with-mysqli=shared,%p/bin/mysql_config \
  --with-pspell=shared,%p \
  --with-unixODBC=shared,%p \
  --with-recode=shared,%p \
  --with-xsl=shared,%p \
  --with-snmp=shared,%p \
  --with-sqlite=shared,%p \
  --enable-sqlite-utf8 \
  --without-sybase-ct \
  --with-tidy=shared,%p \
  --with-xmlrpc=shared \
  --with-pgsql=shared,%p \
  --enable-pdo=shared \
  --without-pdo-dblib \
  --with-pdo-mysql=shared,%p \
  --with-pdo-odbc=shared,unixODBC,%p \
  --with-pdo-pgsql=shared,%p \
  --with-pdo-sqlite=shared,%p \
  --with-gmp=shared,%p

  cd %b/apache2-build && \
  cp ../Zend/zend_ini_scanner.c ../Zend/zend_language_scanner.c \
     ../Zend/zend_ini_parser.h ../Zend/zend_language_parser.h \
     ../Zend/zend_ini_parser.c ../Zend/zend_language_parser.c \
     Zend/

  ### Configure for cgi
  if [ -d %b/cgi-build ]; then rm -rf %b/cgi-build; fi
  mkdir -p %b/cgi-build
  cd %b/cgi-build && \
  PGSQL_INCLUDE=`%p/bin/pg_config-8.3 --includedir` EXTRA_LIBS="-L%p/lib/system-openssl/lib" YACC="/usr/bin/bison -y" ../configure \
  --enable-force-cgi-redirect \
  --enable-fastcgi \
  --with-config-file-path=%p/etc/php5/cgi \
  --with-config-file-scan-dir=%p/etc/php5/conf.d \
  %c \
  --without-mm \
  --disable-pdo \
  --without-mysql \
  --without-sybase-ct \
  --without-sqlite

  cd %b/cgi-build && \
  cp ../Zend/zend_ini_scanner.c ../Zend/zend_language_scanner.c \
     ../Zend/zend_ini_parser.h ../Zend/zend_language_parser.h \
     ../Zend/zend_ini_parser.c ../Zend/zend_language_parser.c \
     Zend/
 
  ### Configure for cli
  if [ -d %b/cli-build ]; then rm -rf %b/cli-build; fi
  mkdir -p %b/cli-build
  cd %b/cli-build && \
  PGSQL_INCLUDE=`%p/bin/pg_config-8.3 --includedir` EXTRA_LIBS="-L%p/lib/system-openssl/lib" YACC="/usr/bin/bison -y" ../configure \
  --disable-cgi \
  --with-config-file-path=%p/etc/php5/cli \
  --with-config-file-scan-dir=%p/etc/php5/conf.d \
  %c \
  --without-mm \
  --disable-pdo \
  --without-mysql \
  --without-sybase-ct \
  --without-sqlite \
  --enable-pcntl \
  --with-ncurses=%p

  cd %b/cli-build && \
  cp ../Zend/zend_ini_scanner.c ../Zend/zend_language_scanner.c \
     ../Zend/zend_ini_parser.h ../Zend/zend_language_parser.h \
     ../Zend/zend_ini_parser.c ../Zend/zend_language_parser.c \
     Zend/

  ### Make missing dirs
  mkdir -p %b/apache2-build/ext/sqlite/libsqlite/src
  mkdir -p %b/cli-build/ext/sqlite/libsqlite/src
  mkdir -p %b/cgi-build/ext/sqlite/libsqlite/src

  ### Fix for dlcompat
#  perl -pi -e 's,#define HAVE_MACH_O_DYLD_H 1,#undef HAVE_MACH_O_DYLD_H,g' apache2-build/main/php_config.h

  ### Fix for dlcompat
#  perl -pi -e 's,#define HAVE_MACH_O_DYLD_H 1,#undef HAVE_MACH_O_DYLD_H,g' cgi-build/main/php_config.h

  ### Force system-openssl
  perl -pi -e 's,LIBADD =,LIBADD = -L%p/lib/system-openssl/lib,g' %b/apache2-build/Makefile
  perl -pi -e 's,LIBADD =,LIBADD = -L%p/lib/system-openssl/lib,g' %b/cli-build/Makefile
  perl -pi -e 's,LIBADD =,LIBADD = -L%p/lib/system-openssl/lib,g' %b/cgi-build/Makefile
  perl -pi -e 's,PHP_LDFLAGS =,PHP_LDFLAGS = -L%p/lib/system-openssl/lib,g' %b/apache2-build/Makefile
  perl -pi -e 's,PHP_LDFLAGS =,PHP_LDFLAGS = -L%p/lib/system-openssl/lib,g' %b/cli-build/Makefile
  perl -pi -e 's,PHP_LDFLAGS =,PHP_LDFLAGS = -L%p/lib/system-openssl/lib,g' %b/cgi-build/Makefile
  perl -pi -e 's,EXTRA_LDFLAGS_PROGRAM =,EXTRA_LDFLAGS_PROGRAM = -L%p/lib/system-openssl/lib,g' %b/apache2-build/Makefile
  perl -pi -e 's,EXTRA_LDFLAGS_PROGRAM =,EXTRA_LDFLAGS_PROGRAM = -L%p/lib/system-openssl/lib,g' %b/cli-build/Makefile
  perl -pi -e 's,EXTRA_LDFLAGS_PROGRAM =,EXTRA_LDFLAGS_PROGRAM = -L%p/lib/system-openssl/lib,g' %b/cgi-build/Makefile
  perl -pi -e 's,EXTRA_LDFLAGS =,EXTRA_LDFLAGS = -L%p/lib/system-openssl/lib,g' %b/apache2-build/Makefile
  perl -pi -e 's,EXTRA_LDFLAGS =,EXTRA_LDFLAGS = -L%p/lib/system-openssl/lib,g' %b/cli-build/Makefile
  perl -pi -e 's,EXTRA_LDFLAGS =,EXTRA_LDFLAGS = -L%p/lib/system-openssl/lib,g' %b/cgi-build/Makefile
  perl -pi -e 's,CFLAGS_CLEAN =,CFLAGS_CLEAN = -I%p/lib/system-openssl/include,g' %b/apache2-build/Makefile
  perl -pi -e 's,CFLAGS_CLEAN =,CFLAGS_CLEAN = -I%p/lib/system-openssl/include,g' %b/cli-build/Makefile
  perl -pi -e 's,CFLAGS_CLEAN =,CFLAGS_CLEAN = -I%p/lib/system-openssl/include,g' %b/cgi-build/Makefile
  perl -pi -e 's,INCLUDES =,INCLUDES = -I%p/lib/system-openssl/include,g' %b/apache2-build/Makefile
  perl -pi -e 's,INCLUDES =,INCLUDES = -I%p/lib/system-openssl/include,g' %b/cli-build/Makefile
  perl -pi -e 's,INCLUDES =,INCLUDES = -I%p/lib/system-openssl/include,g' %b/cgi-build/Makefile

  ### Build for apache2
  cd %b/apache2-build && make

  ### Build for cgi
  cd %b/cgi-build && make && mv sapi/cgi/php-cgi sapi/cgi/cgi-bin.php5
  # Dirty hack to not rebuild everything twice
  cd %b/cgi-build/main && \
    sed -i -e 's/FORCE_CGI_REDIRECT 1/FORCE_CGI_REDIRECT 0/' \
           -e 's/DISCARD_PATH 0/DISCARD_PATH 1/' php_config.h && \
    sed -i -e 's/--enable-force-cgi-redirect/--enable-discard-path/' build-defs.h && \
    touch ../../ext/standard/info.c && \
    touch ../../sapi/cgi/cgi_main.c
  cd %b/cgi-build && make && mv sapi/cgi/php-cgi sapi/cgi/usr.bin.php5-cgi

  ### Build for cli
  cd %b/cli-build && make

  ### Build for pear
  if [ -d %b/pear-build ]; then rm -rf %b/pear-build; fi
  mkdir -p %b/pear-build
  cd %b/cgi-build && make install-pear PHP_PEAR_PHP_BIN=%p/bin/php5 PHP_PEAR_INSTALL_DIR=%p/share/php5 PHP_PEAR_SYSCONF_DIR=%p/etc/pear PHP_PEAR_SIG_BIN=%p/bin/gpg INSTALL_ROOT=%b/pear-build
  sed -i -e 's/-d output_buffering=1 -d open_basedir="" -d safe_mode=0/-d output_buffering=1 -d open_basedir="" -d safe_mode=0 -d memory_limit="-1"/' \
         %b/pear-build%p/bin/pear
  sed -i -e 's/-d output_buffering=1 -d safe_mode=0/-d output_buffering=1 -d open_basedir="" -d safe_mode=0 -d memory_limit="-1"/' \
         %b/pear-build%p/bin/pecl
  sed -i -e 's/-d memory_limit="-1"//' %b/pear-build%p/bin/peardev
  sed -i -e 's/-d output_buffering=1 -d open_basedir="" -d safe_mode=0/-d output_buffering=1 -d open_basedir="" -d safe_mode=0 -d memory_limit="-1"/' \
         %b/pear-build%p/bin/peardev
<<
