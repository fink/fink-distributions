NoSetMAKEFLAGS: true
SetMAKEFLAGS: -j1
SetCPPFLAGS: -I%p/lib/fontconfig2/include -I%p/include/db4
SetLDFLAGS: -L%p/lib/fontconfig2/lib -L%p/lib/seamonkey
SetCFLAGS: -Os
PatchFile: %n.patch
PatchFile-MD5: ed7129666bac7dadc5c602239cdd554c
PatchScript: <<
 %{default_script}

# No idea what upstream mis-assumption this fixes...
 perl -pi -e 's,<arpa/nameser.h>,$&\n#include <arpa/nameser_compat.h>,' servers/exchange/lib/e2k-autoconfig.c

#Make quite sure we don't see this old thing
	mv libdb libdb_UNUSED

	perl -pi -e 's,/usr/share/gtk-doc/html,%p/share/gtk-doc/html,g' docs/reference/*/html/*.html docs/reference/*/*/html/*.html
<<
ConfigureParams: <<
	--disable-dependency-tracking --libexecdir=%p/sbin  --enable-nss=yes --enable-smime=yes --with-nspr-libs=%p/lib/seamonkey \
	--with-nss-libs=%p/lib/seamonkey --with-nspr-includes=%p/include/seamonkey/nspr --with-nss-includes=%p/include/seamonkey/nss \
	--with-krb4=/usr --with-krb5=/usr --enable-gnome-keyring=yes --enable-nntp=yes --with-openldap=yes --with-libdb=%p \
	--disable-static --disable-gtk-doc --enable-compile-warnings=yes \
	PKG_CONFIG_PATH="%p/lib/fontconfig2/lib/pkgconfig:$PKG_CONFIG_PATH"
<<
# trouble with --enable-cxx
CompileScript: <<
	autoreconf -fi
	./configure %c
	make
<<
InstallScript: <<
	make -j1 install DESTDIR="%d"
<<

