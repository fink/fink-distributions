+  // expected-error@+1 {{private variable cannot be lastprivate}} expected-note@+1 {{defined as private}}
+  #pragma omp for simd private(x), lastprivate(x)
+  for (i = 0; i < 16; ++i);
+
+  #pragma omp for simd safelen(4) reduction(+:x, y), reduction(-:z)
+  for (i = 0; i < 16; ++i);
