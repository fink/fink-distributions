+  #pragma omp for simd aligned(,
+  for (i = 0; i < 16; ++i) ;
+  /* expected-error@+1 2 {{expected expression}} */
+  #pragma omp for simd aligned(,)
+  for (i = 0; i < 16; ++i) ;
+  /* expected-error@+1 {{expected expression}} */
+  #pragma omp for simd aligned()
+  for (i = 0; i < 16; ++i) ;
+  /* expected-error@+1 {{expected expression}} */
