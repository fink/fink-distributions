+  const CapturedStmt *Cap = dyn_cast_or_null<CapturedStmt>(getAssociatedStmt());
+  if (!Cap)
+    return 0;
+  const ForStmt *For = dyn_cast_or_null<ForStmt>(Cap->getCapturedStmt());
