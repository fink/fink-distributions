+// CHECK: T res;
+#pragma omp target
+#pragma omp teams distribute parallel for lastprivate(res) if(m_a)
+// CHECK-NEXT: #pragma omp target
+// CHECK-NEXT: #pragma omp teams distribute parallel for lastprivate(res) if(this->m_a)
