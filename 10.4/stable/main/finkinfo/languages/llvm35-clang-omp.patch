+  #pragma omp for simd aligned(x, y:8)
+  for (i = 0; i < 16; ++i) ;
+  /* expected-error@+1 {{argument of an aligned clause should be array, pointer, reference to array or reference to pointer}} */
+  #pragma omp for simd aligned(x, y, z:10+6)
+  for (i = 0; i < 16; ++i) ;
