+void CodeGenFunction::EmitUniversalStore(llvm::Value *Dst, llvm::Value *Src,
+    QualType ExprTy) {
+  EmitUniversalStore(MakeNaturalAlignAddrLValue(Dst, ExprTy), Src, ExprTy);
+}
