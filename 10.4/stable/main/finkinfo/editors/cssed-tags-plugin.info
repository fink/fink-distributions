PatchScript: <<
 #!/bin/sh -ev
 perl -pi.bak -e 's|powerpc-apple-darwin\*\)|\*-apple-darwin\*\)|' configure.in
 perl -pi.bak -e 's,#include <gtk/gtk.h>,#include <strings.h>\n#include <gtk/gtk.h>,g' src/callbacks.c
 aclocal && automake --gnu --add-missing && autoconf
