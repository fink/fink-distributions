+		for arg in ${COMP_WORDS} ; do
+			if [[ "$is_project_next" = "true" ]]; then
+				project_args="-project $arg"
+				break
+			elif [[ "$arg" = "-project" ]]; then
+				is_project_next="true"
+			fi
+		done
