+		local IFS=$'\n'
+		COMPREPLY=( $( echo "$projects" | grep "^$cur" ) )
+	elif [[ "$prev" = "-target" ]]; then
+		local arg project_args is_project_next
+		is_project_next="false"
+		for arg in ${COMP_WORDS} ; do
