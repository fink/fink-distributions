+FORTRAN_SUBR ( LRINFO, lrinfo,
+	       (int *mindx, fpstr versnx, int *ncolx, int *nreflx, float *ranges, int versnx_len),
+	       (int *mindx, fpstr versnx, int *ncolx, int *nreflx, float *ranges),
