+     for (k = 0; k < mtzdata[*mindx-1]->xtal[i]->set[j]->ncol; ++k) {
+       ++icol;
+       for (l = 0; l < 5; ++l)
+         if (sortx[l] == icol)
+           colsort[l] = mtzdata[*mindx-1]->xtal[i]->set[j]->col[k];
