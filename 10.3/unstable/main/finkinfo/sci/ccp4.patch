+  ccp4_CtoFString(FTN_STR(versnx),FTN_LEN(versnx),MTZVERSN);
+  *ncolx = MtzNumActiveCol(mtzdata[*mindx-1]);
+  *nreflx = MtzNref(mtzdata[*mindx-1]);
