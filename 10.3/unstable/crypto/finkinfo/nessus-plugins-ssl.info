#NoSetCFLAGS: true
#NoSetCPPFLAGS: true
#SetCFLAGS: -isystem %p/include -I%p/include/sys
#SetCPPFLAGS: -isystem %p/include -I%p/include/sys
#SetLIBS: -lpoll -ldl
