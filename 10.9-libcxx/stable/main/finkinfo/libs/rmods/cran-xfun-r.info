Depends:  <<
	fink (>= 0.27.2),
	r-base%type_pkg[rversion],
	libgettext8-shlibs
<<
BuildDepends: <<
	fink (>= 0.27.2),
	r-base%type_pkg[rversion]-dev,
	libgettext8-dev
<<
CompileScript: <<
  #!/bin/bash -ev
  export TMPDIR=%b/tmp
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  pushd ..
  $BIN_R --verbose CMD build --no-build-vignettes xfun
<<
InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && $BIN_R --verbose CMD INSTALL --library=%i/lib/R/%type_raw[rversion]/site-library xfun
  if [ "%type_num[rversion]" -lt "34" ]; then
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/xfun/libs/xfun.dylib %i/lib/R/%type_raw[rversion]/site-library/xfun/libs/xfun.dylib
  else
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/xfun/libs/xfun.so %i/lib/R/%type_raw[rversion]/site-library/xfun/libs/xfun.so
  fi
<<
