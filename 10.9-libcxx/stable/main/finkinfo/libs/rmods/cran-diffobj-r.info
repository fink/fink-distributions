SourceDirectory: diffobj
CustomMirror: <<
	Primary: https://cran.r-project.org/src/contrib
	Secondary: https://cran.r-project.org/src/contrib/00Archive/diffobj
<<
Depends: <<
	r-base%type_pkg[rversion],
	cran-crayon-r%type_pkg[rversion] (>= 1.3.2-1),
	(%type_raw[rversion] << 3.6) gcc5-shlibs | (%type_raw[rversion] >= 3.6) gcc9-shlibs,
	libgettext8-shlibs
<<
BuildDepends: <<
	fink (>= 0.27.2),
	r-base%type_pkg[rversion]-dev,
	(%type_raw[rversion] << 3.6) gcc5-compiler | (%type_raw[rversion] >= 3.6) gcc9-compiler,
	libgettext8-dev
<<
CompileScript: <<
  #!/bin/bash -ev
  export TMPDIR=%b/tmp
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  pushd ..
  $BIN_R --verbose CMD build --no-build-vignettes diffobj
<<
InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && `which xvfb-run` $BIN_R --verbose CMD INSTALL --library=%i/lib/R/%type_raw[rversion]/site-library diffobj
  if (( $(echo "%type_raw[rversion] > 3.3" |bc -l) )); then
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/diffobj/libs/diffobj.so %i/lib/R/%type_raw[rversion]/site-library/diffobj/libs/diffobj.so
  else
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/diffobj/libs/diffobj.dylib %i/lib/R/%type_raw[rversion]/site-library/diffobj/libs/diffobj.dylib
  fi
<<
Shlibs: <<
  ( %type_raw[rversion] >= 3.4 ) %p/lib/R/%type_raw[rversion]/site-library/diffobj/libs/diffobj.so 0.0.0 %n (>= 0.3.4-1)
  ( %type_raw[rversion] << 3.4 ) %p/lib/R/%type_raw[rversion]/site-library/diffobj/libs/diffobj.dylib 0.0.0 %n (>= 0.3.4-1)
<<
DescDetail: <<
  Generate a colorized diff of two R objects for an intuitive
visualization of their differences.
