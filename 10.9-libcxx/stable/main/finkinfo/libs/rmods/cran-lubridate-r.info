	fink (>= 0.27.2), 
	( %type_raw[rversion] >= 3.3 ) r-base%type_pkg[rversion]-dev,
	( %type_raw[rversion] = 3.2 ) r-base%type_pkg[rversion]-dev (>= 3.2.0-2),
