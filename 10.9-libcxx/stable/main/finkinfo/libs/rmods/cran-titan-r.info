CustomMirror: <<
	Primary: https://cran.r-project.org/src/contrib
	Secondary: https://cran.r-project.org/src/contrib/00Archive/titan
<<
Depends: <<
	r-base%type_pkg[rversion],
	cran-boot-r%type_pkg[rversion],
	cran-lattice-r%type_pkg[rversion],
	cran-mass-r%type_pkg[rversion],
	tcltk
<<
BuildDepends: <<
	r-base%type_pkg[rversion]-dev,
	xvfb-run
<<
CompileScript: <<
  #!/bin/bash -ev
  export TMPDIR=%b/tmp
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  pushd ..
  $BIN_R --verbose CMD build --no-build-vignettes titan
<<
InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && `which xvfb-run` $BIN_R --verbose CMD INSTALL --library=%i/lib/R/%type_raw[rversion]/site-library titan
<<
DescDetail: <<
