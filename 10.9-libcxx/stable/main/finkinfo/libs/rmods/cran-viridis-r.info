	fink (>=0.32),
	r-base%type_pkg[rversion],
	cran-ggplot2-r%type_pkg[rversion] (>= 1.0.1),
	cran-gridextra-r%type_pkg[rversion],
	cran-viridislite-r%type_pkg[rversion] (>= 0.3.0)
