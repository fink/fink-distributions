	( %type_raw[rversion] >= 3.4 ) r-base%type_pkg[rversion]-dev (>= 3.4.0-2),
	( %type_raw[rversion] = 3.3 ) r-base%type_pkg[rversion]-dev (>= 3.3.2-2),
