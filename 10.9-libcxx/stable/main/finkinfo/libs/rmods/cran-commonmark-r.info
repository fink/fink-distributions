CompileScript: <<
  #!/bin/bash -ev
  export TMPDIR=%b/tmp
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  pushd ..
  $BIN_R --verbose CMD build --no-build-vignettes commonmark
<<
InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && $BIN_R --verbose CMD INSTALL --library=%i/lib/R/%type_raw[rversion]/site-library commonmark
  if (( $(echo "%type_raw[rversion] > 3.3" |bc -l) )); then
  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/commonmark/libs/commonmark.so %i/lib/R/%type_raw[rversion]/site-library/commonmark/libs/commonmark.so
  else
  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/commonmark/libs/commonmark.dylib %i/lib/R/%type_raw[rversion]/site-library/commonmark/libs/commonmark.dylib
  fi
<<
