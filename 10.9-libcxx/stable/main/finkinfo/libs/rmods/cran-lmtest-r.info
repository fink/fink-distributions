CompileScript: <<
  #!/bin/bash -ev
  export TMPDIR=%b/tmp
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  pushd ..
  $BIN_R --verbose CMD build --no-build-vignettes lmtest
<<
InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && $BIN_R --verbose CMD INSTALL --library=%i/lib/R/%type_raw[rversion]/site-library lmtest
  if [ "%type_num[rversion]" -lt "34" ]; then
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/lmtest/libs/lmtest.dylib %i/lib/R/%type_raw[rversion]/site-library/lmtest/libs/lmtest.dylib
  else
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/lmtest/libs/lmtest.so %i/lib/R/%type_raw[rversion]/site-library/lmtest/libs/lmtest.so
  fi
