Revision: 1
Description: GUI for the reshape2 and plyr packages
Homepage: https://cran.r-project.org/package=reshapeGUI
License: GPL
Maintainer: None <fink-devel@lists.sourceforge.net>
Source: mirror:custom:reshapeGUI_%v.tar.gz
Source-Checksum: SHA256(fa0fbc16ff8623e7a5960e6e0738860000c40c78346d93a905fc21cd753c654d)
SourceDirectory: reshapeGUI
CustomMirror: <<
	Primary: https://cran.r-project.org/src/contrib
	Secondary: https://cran.r-project.org/src/contrib/00Archive/reshapeGUI
<<
Depends: <<
	r-base%type_pkg[rversion],
	cran-gwidgets-r%type_pkg[rversion],
	cran-gwidgetsrgtk2-r%type_pkg[rversion],
	cran-plyr-r%type_pkg[rversion],
	cran-reshape2-r%type_pkg[rversion]
<<
BuildDepends: <<
	r-base%type_pkg[rversion]-dev,
	xvfb-run
<<
CompileScript: <<
  #!/bin/bash -ev
  export TMPDIR=%b/tmp
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  pushd ..
  $BIN_R --verbose CMD build --no-build-vignettes reshapeGUI
<<
InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && `which xvfb-run` $BIN_R --verbose CMD INSTALL --library=%i/lib/R/%type_raw[rversion]/site-library reshapeGUI
<<
