CompileScript: <<
  #!/bin/bash -ev
  export TMPDIR=%b/tmp
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  pushd ..
  $BIN_R --verbose CMD build --no-build-vignettes promises
<<
InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && $BIN_R --verbose CMD INSTALL --library=%i/lib/R/%type_raw[rversion]/site-library promises
  if [ "%type_num[rversion]" -lt "34" ]; then
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/promises/libs/promises.dylib %i/lib/R/%type_raw[rversion]/site-library/promises/libs/promises.dylib
  else
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/promises/libs/promises.so %i/lib/R/%type_raw[rversion]/site-library/promises/libs/promises.so
  fi
<<
Shlibs: <<
