InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && $BIN_R --verbose CMD INSTALL --library=%i/lib/R/%type_raw[rversion]/site-library pan
  if [ "%type_num[rversion]" -lt "34" ]; then
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/pan/libs/pan.dylib %i/lib/R/%type_raw[rversion]/site-library/pan/libs/pan.dylib
  else
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/pan/libs/pan.so %i/lib/R/%type_raw[rversion]/site-library/pan/libs/pan.so
  fi
<<
