	fink (>= 0.27.2), 
	r-base%type_pkg[rversion],
	cran-aws-r%type_pkg[rversion] (>= 2.4-1),
	cran-nlme-r%type_pkg[rversion],
	cran-metafor-r%type_pkg[rversion],
	(%type_raw[rversion] << 3.6) gcc5-shlibs | (%type_raw[rversion] >= 3.6) gcc9-shlibs,
