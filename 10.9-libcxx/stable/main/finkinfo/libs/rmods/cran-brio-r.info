CompileScript: <<
  #!/bin/bash -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  pushd ..
  $BIN_R --verbose CMD build --no-build-vignettes brio
<<
InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && `which xvfb-run` $BIN_R --verbose CMD INSTALL --library=%i/lib/R/%type_raw[rversion]/site-library brio
  if (( $(echo "%type_raw[rversion] > 3.3" |bc -l) )); then
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/brio/libs/brio.so %i/lib/R/%type_raw[rversion]/site-library/brio/libs/brio.so
  else
	  install_name_tool -id %p/lib/R/%type_raw[rversion]/site-library/brio/libs/brio.dylib %i/lib/R/%type_raw[rversion]/site-library/brio/libs/brio.dylib
  fi
<<
