BuildDepends: <<
	fink (>= 0.27.2),
	r-base%type_pkg[rversion]-dev,
	cran-rcpp-r%type_pkg[rversion]-dev (>= 0.12.3-1),
	cran-rcpparmadillo-r%type_pkg[rversion]-dev,
