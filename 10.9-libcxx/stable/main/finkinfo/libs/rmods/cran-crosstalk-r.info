Version: 1.0.0
Revision: 3
Description: Inter-widget interactivity for HTML widgets
Homepage: http://cran.r-project.org/web/packages/crosstalk/index.html
License: BSD
Maintainer: None <fink-devel@lists.sourceforge.net>
Source: http://cran.r-project.org/src/contrib/crosstalk_%v.tar.gz
Source-MD5: c13c21b81af2154be3f08870fd3a7077
SourceDirectory: crosstalk
Depends: <<
	fink (>=0.32),
	r-base%type_pkg[rversion],
	cran-htmltools-r%type_pkg[rversion] (>=0.3.5),
	cran-ggplot2-r%type_pkg[rversion],
	cran-jsonlite-r%type_pkg[rversion],
	cran-lazyeval-r%type_pkg[rversion],
	cran-r6-r%type_pkg[rversion],
	cran-shiny-r%type_pkg[rversion] (>=0.11)
