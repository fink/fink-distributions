	Primary: https://cran.r-project.org/src/contrib
	Secondary: https://cran.r-project.org/src/contrib/00Archive/rematch2
<<
Depends: <<
	r-base%type_pkg[rversion],
	cran-tibble-r%type_pkg[rversion]
<<
BuildDepends: <<
	fink (>= 0.27.2),
	r-base%type_pkg[rversion]-dev
<<
CompileScript: <<
  #!/bin/bash -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  pushd ..
  $BIN_R --verbose CMD build --no-build-vignettes rematch2
<<
InstallScript: <<
  #!/bin/sh -ev
  BIN_R=%p/Library/Frameworks/R.framework/Versions/%type_raw[rversion]/Resources/bin/R
  
  mkdir -p %i/lib/R/%type_raw[rversion]/site-library
  pushd %b/.. && `which xvfb-run` $BIN_R --verbose CMD INSTALL --library=%i/lib/R/%type_raw[rversion]/site-library rematch2
<<
DescDetail: <<
  Wrappers on 'regexpr' and 'gregexpr' to return the match results 
in tidy data frames.
<<

