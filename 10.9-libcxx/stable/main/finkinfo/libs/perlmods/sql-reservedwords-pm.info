UpdatePOD: true
InstallScript: <<
	%{default_script}
	mv %i/share/man %i/lib/perl5/%type_raw[perl]
	mv %i/bin/sqlrw %i/bin/sqlrw-%type_raw[perl]
<<
DocFiles: Changes README

PostInstScript: <<
if [ "$1" = configure ]
then
