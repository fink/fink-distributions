Depends: <<
	aniso8601-py%type_pkg[python] (>= 0.82),
	flask-py%type_pkg[python] (>= 0.8),
	python%type_pkg[python],
	pytz-py%type_pkg[python],
	six-py%type_pkg[python] (>= 1.3.0)
<<
