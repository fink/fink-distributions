	#!/bin/sh -ev
	## X11 only patches
	#if [ "%type_raw[qt]" == "x11" ]; then
	#	patch -p1 < %{PatchFile}
	#fi
	# remove flags fatal to Xcode12 that're triggered by clang no
	# longer reporting as v4.2.1
	perl -pi -e 's|-fno-delete-null-pointer-checks -fno-lifetime-dse||g' `grep -rl fno-lifetime-dse *`
<<
