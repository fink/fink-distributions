	### Make sure we don't find X11/Freetype on 10.7
	perl -pi -e 's|CFLAGS\=\"\$FREETYPE_CFLAGS \$CFLAGS\"|CPPFLAGS\=\"\$FREETYPE_CFLAGS \$CPPFLAGS\"|g' configure
