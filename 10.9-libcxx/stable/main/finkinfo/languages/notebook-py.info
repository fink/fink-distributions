BuildDepends: python%type_pkg[python], setuptools-tng-py%type_pkg[python], pandoc

CompileScript: <<
 #!/bin/sh -ev
 python%type_raw[python] setup.py build
 cd docs
 perl -pi -e 's|python3|python%type_raw[python]|;' Makefile
 perl -pi -e 's|jupyter nbconvert|jupyter-nbconvert%type_raw[python]|;' Makefile
 make LC_ALL=C PYTHONPATH=../build/lib PYTHON=%p/bin/python%type_raw[python] SPHINXBUILD=%p/bin/sphinx-build%type_raw[python] html
 find .. -name '*.py[oc]' -exec rm {} \;
<<

InfoTest: <<
 TestDepends: <<
  nose-py%type_pkg[python],
  nose-warnings-filters-py%type_pkg[python],
  requests-py%type_pkg[python],
  selenium-py%type_pkg[python],
  ( %type_pkg[python] <= 27 ) mock-py%type_pkg[python]
 <<
 TestScript: <<
  # disable selenium tests failing with missing required argument in test_*() calls
  PYTHONPATH=%b/build/lib %p/bin/python%type_raw[python] -m nose -e test_items -e test_delete_cells -e test_insert_cell -e test_markdown_cell -e test_save_notebook_as
 <<
 TestSuiteSize: medium
<<

InstallScript: <<
 #!/bin/bash -ev
 find build -name '*.py[oc]' -exec rm {} \;
 %p/bin/python%type_raw[python] setup.py install --root=%d
 for file in %i/bin/*; do
   echo mv $file ${file}-py%type_pkg[python]
   mv $file ${file}-py%type_pkg[python]
 done
<<

PostInstScript: <<
  # Remove the old if they exist
  update-alternatives --auto jupyter-notebook

  # Now the new setup
  update-alternatives --verbose --install %p/bin/jupyter-notebook jupyter-notebook %p/bin/jupyter-notebook-py%type_pkg[python] %type_pkg[python] \
	--slave %p/bin/jupyter-bundlerextension jupyter-bundlerextension %p/bin/jupyter-bundlerextension-py%type_pkg[python] \
	--slave %p/bin/jupyter-nbextension jupyter-nbextension %p/bin/jupyter-nbextension-py%type_pkg[python] \
	--slave %p/bin/jupyter-serverextension jupyter-serverextension %p/bin/jupyter-serverextension-py%type_pkg[python]

  echo "The scripts 'jupyter-notebook', 'jupyter-bundlerextension',"
  echo "'jupyter-nbextension' and 'jupyter-serverextension' installed in %p/bin/"
  echo "are symlinks automatically updated by the update-alternatives utility."
  echo "If you install multiple flavours of notebook, the symlinks"
  echo "will point to the higher flavour by default."
  echo "You can inspect the links by running:"
  echo "  update-alternatives --list jupyter-notebook"
  echo "  update-alternatives --display jupyter-notebook"
<<

PreRmScript: <<
  # Remove the old
  if [ $1 != "upgrade" ]; then
     update-alternatives --verbose --remove jupyter-notebook %p/bin/jupyter-notebook%type_raw[python]
  else
    echo "Do not remove during upgrade"
  fi
<<

