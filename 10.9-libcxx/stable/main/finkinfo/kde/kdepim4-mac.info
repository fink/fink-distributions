CompileScript: <<
#!/bin/sh -ev
	export KDE4_PREFIX="%p" KDE4_TYPE="%type_pkg[kde]"
	. %p/sbin/kde4.12-buildenv.sh

	export CC="$CC -MD" CXX="$CXX -MD"
	mkdir -p build
	pushd build
	cmake \
		$KDE4_CMAKE_ARGS \
		-DGPG_ERROR_INCLUDE_DIR:PATH=%p/include \
		-DBUNDLE_INSTALL_DIR=%p/Applications/KDE4 \
	..
	### This library is misidentified somehow
	perl -pi -e 's|-lKDEPIMLibs__gpgmepp|-lgpgme++|g' kmail/kcm_kpimidentities/CMakeFiles/kcm_kpimidentities.dir/link.txt
	make VERBOSE=1
	popd
	fink-package-precedence --depfile-ext='\.d' --prohibit-bdep=kdepim4-%type_pkg[kde]-dev .
<<

