  ./stressRooFit      || exit 2
  ./stressShapes      || exit 2
  ./stressSpectrum    || exit 2
  ./stressTMVA        || exit 2
  ./stressVector      || exit 2
  # Cleanup
  make distclean
  /bin/mv -f Makefile.bak Makefile
  /bin/rm -f .rootrc .rootrc.bak .rootmap root.mimes sg* vector.dll vectorbool.dll AutoDict_vector_MyClass__.cxx
  /bin/rm -rf weights xpdtut files
 <<
