# Fix/work around python3ism in python2 code. See:
# https://github.com/OSGeo/grass/issues/1241
PatchFile2: %{ni}-python27.patch
PatchFile2-MD5: 02596fd1a510d9ff724c9b9a840f45dd

# Patch Phase.
PatchScript: <<
#!/bin/bash -xe
  
  patch -p1 < %{PatchFile}
  patch -p1 < %{PatchFile2}
  
  # force to use Fink's python
  # this may not be necessary, but for consistency...
  grep -r -I -s "/usr/bin/env python" * | cut -d: -f1 | while
  read -r line ; do
    perl -pi -e "s|/usr/bin/env python\d*|%p/bin/python%type_raw[py_ver]|" $line
  done
  perl -pi -e 's|python3|%p/bin/python%type_raw[py_ver]|' include/Make/Platform.make.in lib/init.grass.{py,sh} lib/python/script/setup.py
  
  # grass.sh.in
  perl -pi -e "s|\@PREFIX\@|%p|" macosx/app/grass.sh.in
  perl -pi -e "s|\@GUI\@|%type_raw[gui]|" macosx/app/grass.sh.in
  perl -pi -e "s|\@PY_VER\@|%type_raw[py_ver]|" macosx/app/grass.sh.in

  # rewrite python wrapper
  echo "exec \"%p/bin/python%type_raw[py_ver]\" \"\$@\"" >macosx/app/python_wrapper
  chmod 0755 macosx/app/python_wrapper

  # patch *ancient* darwin-ignorant autoconf
  perl -pi -e 's/(a so sl)/dylib \1/' configure
  # autoconf2.6ish patch for modern XQuartz paths
  perl -pi -e "s|/usr/lpp/Xamples|/opt/X11|" configure
<<

